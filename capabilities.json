{
    "dataRoles": [
        {
            "displayName": "Bild URL",
            "name": "imageurl",
            "kind": "Measure"
        },
        {
            "displayName": "Alt. Text",
            "name": "alttext",
            "kind": "Grouping"
        }
    ],
    "objects": {
        "circle": {
            "properties": {
                "circleColor": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "circleThickness": {
                    "type": {
                        "numeric": true
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "imageurl": {
                        "max": 1
                    },
                    "alttext": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "bind": {
                                "to": "alttext"
                            }
                        }
                    ]
                },
                "values": {
                    "bind": {
                        "to": "imageurl"
                    }
                }
            }
        }
    ],
    "privileges": [
        {
            "name": "WebAccess",
            "essential": true,
            "parameters": [
                "*"
            ]
        }
    ]
}